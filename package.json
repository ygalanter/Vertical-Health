    "enableMultiJS": true,
    "targetPlatforms": [
      "diorite"
    ],
    "watchapp": {
      "watchface": false
    },
    "messageKeys": [
      "dummy"
    ],
    "resources": {
      "media": [
        {
          "characterRegex": "[0-9:]",
          "file": "fonts/LECO 1976 Regular.ttf",
          "name": "LECO_50",
          "type": "font"
        }
      ]
    }
  }
}