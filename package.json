  "version": "1.0.0",
  "keywords": [
    "pebble-app"
  ],
  "private": true,
  "dependencies": {
    "pebble-effect-layer": "^1.1.4"
  },
  "scripts": {
    "build": "pebble build",
    "clean": "pebble clean",
    "wipe": "pebble wipe",
    "start": "pebble clean && pebble wipe && pebble build && pebble install --emulator diorite"
  },
  "pebble": {
    "displayName": "Vertical",
